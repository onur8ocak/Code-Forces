#include <bits/stdc++.h>
using namespace std;
int main() {
    int t,n;
    cin>>t;
    while(t--){
    cin>>n;
    if(n<10)
    cout<<(n%10-1)*10+1<<endl;
    else if(n<100)
    cout<<(n%10-1)*10+3<<endl;
    else if(n<1000)
    cout<<(n%10-1)*10+6<<endl;
    else
    cout<<(n%10-1)*10+10<<endl;
    }
    return 0;
}
