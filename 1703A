// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main(){
    string s;
    int t;
    cin>>t;
    while(t--)
    {
        cin>>s;
        if(s=="yes"||s=="Yes"||s=="YES"||s=="yEs"||s=="yeS"||s=="YEs"||s=="yES"||s=="YeS"||s=="NO"||s=="No"||s=="nO"||s=="no")
        cout<<"YES"<<endl;
        else
        cout<<"NO"<<endl;
    }
    return 0;
}
