#include <stdio.h>

int main() {
   int a=0,d=0,n,i,m,b=0;
   scanf("%d",&n);
   char s[n];
   m=n;
   while(m--){
   scanf("%s",&s[b]);
   b++;
   }
   for(i=0;i<n;i++){
       if(s[i]=='A')
       a++;
       else
       d++;
   }
   if(a>d)
   printf("Anton");
   else if(a<d)
   printf("Danik");
   else
   printf("Friendship");

    return 0;
}
