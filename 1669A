#include <bits/stdc++.h>
using namespace std;
int main() {
    int t,n;
    cin>>t;
    while(t--)
    {
        cin>>n;
        cout<<"Division"<<" ";
        if(n<=1399) cout<<"4";
        else if(n>=1400&&n<=1599) cout<<"3";
        else if(n>=1600&&n<=1899) cout<<"2";
        else cout<<"1";
        cout<<endl;
    }
    return 0;
}
