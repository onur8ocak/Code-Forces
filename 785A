#include <iostream>
using namespace std;
int main() {
    string a;
    int sum=0,n,i;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>a;
        if(a[0]=='T')
        sum+=4;
        if(a[0]=='C')
        sum+=6;
        if(a[0]=='O')
        sum+=8;
        if(a[0]=='D')
        sum+=12;
        if(a[0]=='I')
        sum+=20;
    }
    cout<<sum;
    return 0;
}
