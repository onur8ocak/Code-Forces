// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    string s[100001];
    int n,sum=0;
    cin>>n;
    for(int i=0;i<n;i++)
    cin>>s[i];
    for(int j=0;j<n-1;j++)
    {
        if(s[j]!=s[j+1])
        sum++;
    }
    cout<<sum+1;
    return 0;
}
